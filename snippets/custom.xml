<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="lawnchair"
          fetch="https://github.com/LawnchairLauncher"
          revision="stable" />  

  <remote name="microg"
          fetch="https://github.com/microg"
          revision="master" />

  <!-- Custom launcher -->
  <project path="packages/apps/Lawnchair" name="Lawnchair" remote="lawnchair"/>

  <!-- MicroG -->
  <project path="packages/apps/GmsCore" name="android_packages_apps_GmsCore" remote="microg"/>
  <project path="packages/apps/GsfProxy" name="android_packages_apps_GsfProxy" remote="microg"/>
  <project path="packages/apps/UnifiedNlp" name="android_packages_apps_UnifiedNlp" remote="microg"/>

</manifest>
